@model SCsms.Models.Users

@{
    ViewBag.Title = "账户添加";
    Layout = "~/Views/Shared/_BackAdmin.cshtml";
}

<div style="margin-top: 50px; text-align: center;">
    <form action="/Admin/AccountsAdd" method="post" onsubmit="return checkForm()">
        <table style="border-collapse: collapse; width: 100%;">
            <thead>
                <tr>
                    <th style="border: 1px solid #dee2e6; padding: 10px;">用户编号</th>
                    <th style="border: 1px solid #dee2e6; padding: 10px;">用户名</th>
                    <th style="border: 1px solid #dee2e6; padding: 10px;">密码</th>
                    <th style="border: 1px solid #dee2e6; padding: 10px;">账户类型</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td style="border: 1px solid #dee2e6; padding: 10px;"><input type="text" name="UserID" value="" class="form-control" /></td>
                    <td style="border: 1px solid #dee2e6; padding: 10px;"><input type="text" name="UserName" value="" class="form-control" /></td>
                    <td style="border: 1px solid #dee2e6; padding: 10px;"><input type="text" name="Password" value="" class="form-control" /></td>
                    <td style="border: 1px solid #dee2e6; padding: 10px;"><input type="text" name="UserType" value="" class="form-control" /></td>
                </tr>
            </tbody>

            <tr>
                <th style="text-align:center;">
                    <div class="text-center">
                        <input type="submit" class="btn btn-primary" value="添加" />
                    </div>
                    <h3 style="color:red;">@ViewBag.notice</h3>
                </th>
            </tr>
        </table>
    </form>


</div>
<script>


</script>
